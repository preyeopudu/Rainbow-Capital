              <div class="account-card__body">
                <form action="/<%=user.username%>/deposit" method="POST" class="mt-4">
                  
                  <div class="form-group">
                    <input type="text" name="coupon" class="form-control" placeholder="Input Coupon Code" required>
                  </div>


     
                            <div class="mt-3">
                              <button class="cmn-btn" onclick="save()">Submit</button>
                            </div>
                          </form>
                         
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <!-- account section end -->



  </div> <!-- page-wrapper end -->
